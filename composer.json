{
    "name": "tmaic/laravel-translate",
    "description": "一个集成百度翻译，有道翻译，Google翻译的PHP composer package  for laravel5.* and lumen",
    "type": "library",
    "require": {
        "illuminate/container": "^5.1",
        "tmaic/translate": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "tmaic",
            "email": "421339244@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "tmaic\\LaravelTranslate\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tmaic\\LaravelTranslate\\Test\\": "./tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "tmaic\\LaravelTranslate\\TranslateServiceProvider"
            ],
            "aliases": {
                "Translate": "tmaic\\LaravelTranslate\\Facade\\Translate"
            }
        }
    },
    "minimum-stability":"dev"

}

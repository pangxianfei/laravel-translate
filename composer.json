{
    "name": "tmaic/laravel-translate",
    "description": "一个集成百度翻译，有道翻译，Google翻译的PHP composer package  for laravel8.* and lumen",
    "type": "library",
    "require": {
        "illuminate/container": "^5.1",
        "Tmaic/translate": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tmaic",
            "email": "421339244@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tmaic\\LaravelTranslate\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tmaic\\LaravelTranslate\\Test\\": "./tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Tmaic\\LaravelTranslate\\TranslateServiceProvider"
            ],
            "aliases": {
                "Translate": "Tmaic\\LaravelTranslate\\Facade\\Translate"
            }
        }
    },
    "minimum-stability":"dev"

}
